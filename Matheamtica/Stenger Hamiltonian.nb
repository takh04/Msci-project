Cell[TextData[{
 "\nThe Berry curvature can be calculated with  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["B", "g"], "=", 
    RowBox[{"\[ImaginaryI]", 
     RowBox[{
      SubscriptBox["\[Sum]", 
       RowBox[{"g", "\[NotEqual]", "e"}]], 
      FractionBox[
       RowBox[{
        RowBox[{"\[LeftAngleBracket]", " ", 
         RowBox[{"g", ",", 
          RowBox[{"R", 
           RowBox[{"\[LeftBracketingBar]", 
            RowBox[{
             SubscriptBox["\[Del]", "R"], "H"}], "\[RightBracketingBar]"}], 
           "e"}], ",", "R"}], " ", "\[RightAngleBracket]"}], " ", "\[Cross]", 
        
        RowBox[{"\[LeftAngleBracket]", " ", 
         RowBox[{"g", ",", 
          RowBox[{"R", 
           RowBox[{"\[LeftBracketingBar]", 
            RowBox[{
             SubscriptBox["\[Del]", "R"], "H"}], "\[RightBracketingBar]"}], 
           "e"}], ",", "R"}], " ", "\[RightAngleBracket]"}], " "}], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           SubscriptBox["E", "g"], "(", "R", ")"}], " ", "-", 
          RowBox[{
           SubscriptBox["E", "e"], "(", "R", ")"}]}], ")"}], "2"]], " "}]}]}],
    TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "70f4300f-e63c-4abe-bc73-092a54e4cdfc"],
 ". Starting with the Berry curvature of the even ground state,"
}], "Text",
