Cell[TextData[{
 "\n\nFor the even and odd ground states the Berry Curvature is,\n\t\t\t\t\t\t\
\t\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["B", 
     RowBox[{"g", ",", "even"}]], " ", "=", " ", 
    RowBox[{"-", 
     FractionBox[
      OverscriptBox["d", "^"], 
      RowBox[{"2", 
       SuperscriptBox["d", "2"]}]]}]}], TraditionalForm]],ExpressionUUID->
  "f9a5160b-17ca-441c-b000-4cd38f0a3394"],
 " ,\n\t\t\t\t\t\t\t\t  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["B", 
     RowBox[{"g", ",", " ", "odd", " "}]], "=", " ", 
    FractionBox[
     OverscriptBox["d", "^"], 
