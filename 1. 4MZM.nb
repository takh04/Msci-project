Cell[TextData[{
 "The Berry phase of the ground state can be calculated by, \n\n\t\t\t\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Gamma]", 
     RowBox[{"g", ",", "geo"}]], " ", "=", " ", 
    RowBox[{
     RowBox[{"\[ImaginaryI]", 
      RowBox[{"\[Integral]", 
       RowBox[{
        RowBox[{"\[LeftAngleBracket]", 
         RowBox[{"g", ",", 
          RowBox[{"R", 
           RowBox[{"\[LeftBracketingBar]", 
            SubscriptBox["\[Del]", "R"], "\[RightBracketingBar]"}], "g"}], 
          ",", "R"}], "\[RightAngleBracket]"}], "\[CenterDot]", "dR"}]}]}], 
     "  ", "=", " ", 
     RowBox[{
      RowBox[{"\[Integral]", 
       RowBox[{
        SubscriptBox["A", "g"], "\[CenterDot]", "dR"}]}], "  ", "=", " ", 
      RowBox[{"\[Integral]", 
       RowBox[{
        SubscriptBox["B", "g"], "\[CenterDot]", "dS", "  "}]}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"4ab2a421-2919-49a4-8b80-4a632f60556a"],
 "\n\t\t\t\t\t\t\t\t",
 Cell[BoxData[
  FormBox[
